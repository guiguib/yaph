<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:HTMLPanel styleName="row">
		<g:Label ui:field="title" styleName="col s4"></g:Label>
		<g:Label ui:field="value" styleName="col s6"></g:Label>
		<g:HTMLPanel styleName="col s2" ui:field="enclair">
			<g:Anchor ui:field="enclairAction"
				styleName="btn-floating waves-effect waves-light">
				<i class="mdi-action-visibility"></i>
			</g:Anchor>
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder> 