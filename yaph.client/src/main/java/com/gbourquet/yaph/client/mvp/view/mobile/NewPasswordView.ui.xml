<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
		<g:DialogBox ui:field="dialog" visible="false">
			<g:HTMLPanel styleName="bloc">
				<h4><g:Label ui:field="header">New Password</g:Label></h4>
				<g:Label ui:field="error"></g:Label>
				<g:TextBox ui:field="title" maxLength="80" width="400px"></g:TextBox>
				<g:VerticalPanel ui:field="fieldsPanel"></g:VerticalPanel>
				<g:Button ui:field="addFieldButton">Ajouter un champ</g:Button>
				<br /><br />
				<g:HorizontalPanel>
					<g:Button ui:field="validButton">Valider</g:Button>
					<g:Button ui:field="cancelButton">Annuler</g:Button>
				</g:HorizontalPanel>
			</g:HTMLPanel>
		</g:DialogBox>
</ui:UiBinder> 