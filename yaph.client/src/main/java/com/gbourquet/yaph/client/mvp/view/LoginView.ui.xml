<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="bloc">
		<h4>Sign In</h4>	
		<g:Label ui:field="error"></g:Label>
		<g:TextBox ui:field="login" maxLength="20" width="300px"></g:TextBox>
		<g:PasswordTextBox ui:field="passwd" maxLength="20" width="300px"></g:PasswordTextBox>
		<br/>
		<g:Button ui:field="buttonLogin" />
	</g:HTMLPanel>
	
</ui:UiBinder> 