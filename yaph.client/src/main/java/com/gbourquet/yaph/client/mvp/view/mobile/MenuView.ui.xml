<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<header class="wrapper clearfix">
			<h1 class="title">
				<g:Label ui:field="userLabel"></g:Label>
			</h1>
			<nav>
				<div id="bg-one"></div>
				<div id="bg-two"></div>
				<div id="bg-three"></div>
				<div id="bg-four"></div>
				<ul>
					<li>
						<g:Hyperlink targetHistoryToken="app:" ui:field="appAction">Home</g:Hyperlink>
					</li>
					<li>
						<g:Hyperlink targetHistoryToken="password:" ui:field="passwordAction">Passwords</g:Hyperlink>
					</li>
					<li>
						<g:Hyperlink targetHistoryToken="login:" ui:field="actionLabel">Sign in</g:Hyperlink>
					</li>
				</ul>
			</nav>
			<g:Label ui:field="disconnectedLabel"></g:Label>
		</header>
	</g:HTMLPanel>
</ui:UiBinder> 