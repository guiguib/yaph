<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:SimplePanel styleName="login">
		<g:VerticalPanel>
			<g:Label ui:field="error"></g:Label>
			<g:HTMLPanel styleName="loginPanel">
				<g:FlowPanel>
				<g:TextBox ui:field="login" maxLength="20" width="300px"></g:TextBox>
				<g:PasswordTextBox ui:field="passwd" maxLength="20" width="300px"></g:PasswordTextBox>
				</g:FlowPanel>
			</g:HTMLPanel>
			<g:Button ui:field="buttonLogin" />
		</g:VerticalPanel>
	</g:SimplePanel>
</ui:UiBinder> 