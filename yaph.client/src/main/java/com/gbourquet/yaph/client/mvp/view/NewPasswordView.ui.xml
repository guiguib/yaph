<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="input-field">
		<h4>
			<g:Label ui:field="header">New Password</g:Label>
		</h4>
		<g:Label ui:field="error"></g:Label>
		<g:TextBox ui:field="title" maxLength="80" width="400px"></g:TextBox>
		<g:VerticalPanel ui:field="fieldsPanel"></g:VerticalPanel>
		<g:Button ui:field="addFieldButton" styleName="btn">Ajouter un champ</g:Button>
		<br />
		<br />
		<g:HorizontalPanel>
			<g:Button ui:field="validButton" styleName="btn">Valider</g:Button>
			<g:Button ui:field="cancelButton" styleName="btn">Annuler</g:Button>
		</g:HorizontalPanel>

	</g:HTMLPanel>
</ui:UiBinder> 