<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.cadre {
		border: 1px solid #ccc;
		margin: 10px 0 10px 0;
		padding: 10px;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.cadre}">
		<div class="row">
			<div class="col s3">
				<g:Anchor ui:field="del"
					styleName="btn-floating tooltipped waves-effect waves-light">
					<i class="mdi-content-remove"></i>
				</g:Anchor>
			</div>
			<div class="col s3">&nbsp;
				<g:Anchor ui:field="generate"
					styleName="btn-floating tooltipped waves-effect waves-light">
					<i class="mdi-action-perm-data-setting"></i>
				</g:Anchor>
			</div>
			<div class="col s5">
				<g:ListBox ui:field="type" styleName="browser-default"></g:ListBox>
			</div>
		</div>
		<div class="row">
			<div class="input-field col s6">
				<g:TextBox ui:field="title"></g:TextBox>
			</div>
			<div class="input-field col s6">
				<g:TextBox ui:field="value"></g:TextBox>
			</div>

		</div>


	</g:HTMLPanel>
</ui:UiBinder> 