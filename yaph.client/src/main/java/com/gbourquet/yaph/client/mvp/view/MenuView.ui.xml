<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<nav>
			<div class="nav-wrapper">
				<a href="#" class="brand-logo">
					<g:Label ui:field="userLabel"></g:Label>
				</a>
				<a href="#" data-activates="nav-mobile" class="button-collapse" id="bMenu">
					<i class="mdi-navigation-menu"></i>
				</a>
				<ul id="" class="right hide-on-med-and-down">
					<li>
						<a href="#app:">Home</a>
					</li>
					<li>
						<a href="#password:">Passwords</a>
					</li>
					<li>
						<a href="#login:">Sign in</a>
					</li>
				</ul>
				<ul class="side-nav" id="nav-mobile">
					<li>
						<g:Hyperlink targetHistoryToken="app:" ui:field="appAction">Home</g:Hyperlink>
					</li>
					<li>
						<g:Hyperlink targetHistoryToken="password:" ui:field="passwordAction">Passwords</g:Hyperlink>
					</li>
					<li>
						<g:Hyperlink targetHistoryToken="login:" ui:field="actionLabel">Sign
							in</g:Hyperlink>
					</li>
				</ul>				
			</div>
		</nav>

	</g:HTMLPanel>
</ui:UiBinder> 