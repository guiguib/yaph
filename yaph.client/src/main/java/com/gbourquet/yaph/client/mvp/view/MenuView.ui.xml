<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<nav>
			<div class="nav-wrapper">
				<a href="#" class="brand-logo"><g:Label ui:field="userLabel"></g:Label></a>
				<ul id="nav-mobile" class="right side-nav">
					<li>
						<g:Hyperlink targetHistoryToken="app:" ui:field="appAction">Home</g:Hyperlink>
					</li>
					<li>
						<g:Hyperlink targetHistoryToken="password:" ui:field="passwordAction">Passwords</g:Hyperlink>
					</li>
					<li>
						<g:Hyperlink targetHistoryToken="login:" ui:field="actionLabel">Sign in</g:Hyperlink>
					</li>
				</ul>

				<!-- Include this line below -->
				<a class="button-collapse" href="#" data-activates="nav-mobile"><i class="mdi-navigation-menu"></i></a>
				<!-- End -->

			</div>
		</nav>
	</g:HTMLPanel>
</ui:UiBinder> 