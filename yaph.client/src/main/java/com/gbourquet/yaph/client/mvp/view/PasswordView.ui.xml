<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<g:HTMLPanel styleName="bloc">
		<h4>Passwords</h4>
		<c:DataGrid ui:field='dataGrid' height="55%" pageSize="8" />
		<c:SimplePager ui:field='pager' pageSize="8"/>
		<g:Button ui:field="newPasswordButton">Nouveau</g:Button>
		<g:DialogBox ui:field="newPasswordBox" visible="false">
			<g:HTMLPanel styleName="bloc">
				<h4>New Password</h4>
				<g:Label ui:field="newBoxError"></g:Label>
				<g:TextBox ui:field="newBoxTitre" maxLength="80" width="400px"></g:TextBox>
				<g:TextBox ui:field="newBoxUser" maxLength="80" width="400px"></g:TextBox>
				<g:HorizontalPanel>
					<g:TextBox ui:field="newBoxPassword" maxLength="80" width="250px">
					</g:TextBox><g:Button width="140px" ui:field="newBoxGenerateButton">generate</g:Button>
				</g:HorizontalPanel>
				<g:TextBox ui:field="newBoxUrl" maxLength="200" width="400px"></g:TextBox>
				<br />
				<g:HorizontalPanel>
					<g:Button ui:field="newBoxValidButton">Valider</g:Button>
					<g:Button ui:field="newBoxCancelButton">Annuler</g:Button>
				</g:HorizontalPanel>

			</g:HTMLPanel>
		</g:DialogBox>
	</g:HTMLPanel>

</ui:UiBinder> 