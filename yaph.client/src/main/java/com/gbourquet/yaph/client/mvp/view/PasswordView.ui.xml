<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<g:HTMLPanel styleName="bloc">
		<h4>Passwords</h4>
		<g:DockLayoutPanel height="100%">
			<g:north size="0"><g:HTMLPanel></g:HTMLPanel></g:north>
			<g:north size="50"><g:Button ui:field="newPasswordButton">Nouveau</g:Button></g:north>
			 <g:west size="350">
			    <g:HTMLPanel>
			        West
			    </g:HTMLPanel>
			</g:west>
			<g:center>
				<g:HTMLPanel>
					<c:DataGrid ui:field='dataGrid' height="225px" pageSize="5" />
					<c:SimplePager ui:field='pager' pageSize="5" />
					<g:DisclosurePanel ui:field="detailPassword" height="50%" width="100%" open="false">
						<g:VerticalPanel ui:field="fields"></g:VerticalPanel>
					</g:DisclosurePanel>
				</g:HTMLPanel>
			</g:center>
		</g:DockLayoutPanel>
	</g:HTMLPanel>

</ui:UiBinder> 