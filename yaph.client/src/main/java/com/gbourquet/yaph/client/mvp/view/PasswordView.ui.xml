<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:p="urn:import:com.gbourquet.yaph.client.widget">
	<ui:style>
	.cadre {
		border-radius: 0.35em;
		border: 1px solid #ccc;
		margin: 10px 0 10px 0;
		padding: 10px;
	}
	</ui:style>
	<g:HTMLPanel>
		<h4>Passwords</h4>
		<p:TagsWidget ui:field="tags"></p:TagsWidget>
		<div class="row">
			<div class="col s12">
				<g:Button ui:field="newPasswordButton" styleName="btn">Nouveau</g:Button>
			</div>
		</div>
		<g:HTMLPanel styleName="collapsible" ui:field='data' tag="ul"/>
	</g:HTMLPanel>

</ui:UiBinder> 