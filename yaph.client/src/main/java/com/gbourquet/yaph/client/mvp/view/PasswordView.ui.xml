<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:p="urn:import:com.gbourquet.yaph.client.widget" >
	<g:HTMLPanel styleName="bloc">
		<h4>Passwords</h4>
		<g:DockLayoutPanel height="100%">
			<g:north size="0">
				<g:HTMLPanel></g:HTMLPanel>
			</g:north>
			<g:north size="50">
				<g:Button ui:field="newPasswordButton">Nouveau</g:Button>
			</g:north>
			<g:west size="300">
				<g:HTMLPanel>
					West
				</g:HTMLPanel>
			</g:west>
			<g:center height="100%">
				<g:HTMLPanel>
					<c:DataGrid ui:field='dataGrid' height="185px" pageSize="5" />
					<p:NormalPager ui:field='pager' pageSize="5" />
					<g:SimplePanel ui:field="detailPassword" styleName="detailPassword">
					</g:SimplePanel>
					<g:Button ui:field="updateFields">Modifier</g:Button>
				</g:HTMLPanel>
			</g:center>
		</g:DockLayoutPanel>
	</g:HTMLPanel>

</ui:UiBinder> 